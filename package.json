    }
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint \"**/*.{ts,js}\"",
    "fix": "eslint --fix \"**/*.{ts,js}\"",
    "start": "esno src/index.ts",
    "test": "vitest",
    "release": "np"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.24.2",
    "cac": "^6.7.12",
    "enquirer": "^2.3.6",
    "eslint": "8.16.0",
    "esno": "0.16.3",
    "np": "7.6.1",
    "typescript": "4.6.4",
    "unbuild": "0.7.4",
    "vite": "2.9.9",
    "vitest": "0.13.0"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "consola": "^2.15.3",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "ora": "^6.1.0"
