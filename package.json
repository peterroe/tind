{
  "name": "tind",
  "version": "1.0.0",
  "packageManager": "pnpm@7.1.7",
  "description": "cli-template",
  "keywords": [
    "cli",
    "typescript",
    "template"
  ],
  "license": "MIT",
  "author": "peterroe",
  "module": "./dist/index.mjs",
  "main": "./dist/index.mjs",
  "bin": {
    "tind": "./bin/index.mjs"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
